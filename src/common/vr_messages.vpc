$Project
{
	$Folder "Protobuf Files"
	{
		$File "$SRCDIR\common\vr_messages.proto"
		$DynamicFile "$GENERATED_PROTO_DIR\vr_messages.pb.h"
		$DynamicFile "$GENERATED_PROTO_DIR\vr_messages.pb.cc"
		{
			$Configuration
			{
				$Compiler 
				{
					$Create/UsePrecompiledHeader	"Not Using Precompiled Headers" [$WINDOWS]
				}
			}
		}
	}

}
